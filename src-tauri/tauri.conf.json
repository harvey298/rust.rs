{
  "build": {
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "devPath": "../src",
    "distDir": "../src",
    "withGlobalTauri": true

  },
  "package": {
    "productName": "rustrs",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "window": {
        "all": true
      },
      "shell": {
        "all": false,
        "open": true,
        "sidecar": true,
        "scope": [
          {"name": "B:/etc/projects/rust.rs/src-side-cars/dist/pair_listener", "sidecar": true }
        ]
      }
    },
    "bundle": {
      "active": true,
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "co.uk.hoovy.rustrs",
      "targets": "all",
      "externalBin": [
        "B:/etc/projects/rust.rs/src-side-cars/dist/pair_listener"
      ]
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "label": "main",
        "fullscreen": false,
        "resizable": true,
        "title": "rustrs",
        "width": 800,
        "height": 600
      }


    ]
  }
}
